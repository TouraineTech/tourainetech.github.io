{
  "name": "touraine-tech-website",
  "version": "2.0.0",
  "description": "Touraine Tech conference website",
  "private": true,
  "scripts": {
    "predev": "pnpm run extract:timerDatas",
    "prebuild": "pnpm run extract:timerDatas",
    "pregenerate": "pnpm run extract:timerDatas",
    "dev": "nuxt dev --host 0.0.0.0 --port 3000",
    "build": "nuxt build",
    "start": "nuxt preview",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy": "gh-pages --dotfiles -d dist -b master",
    "planning:fetch": "node tools/fetch.mjs",
    "planning:build": "node tools/build.mjs",
    "planning:validate": "node tools/build.mjs --validate-only",
    "planning:photos": "node tools/photos.mjs",
    "planning:openfeedback": "node tools/generateOpenfeedback.mjs",
    "planning:qrcodes": "node tools/generateQRCodes.mjs",
    "planning:all": "pnpm run planning:build && pnpm run planning:photos && pnpm run planning:openfeedback",
    "extract:timerDatas": "node tools/timerJsonBuilder.mjs",
    "extract:flickr": "node tools/flickr.mjs"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "showdown": "^2.1.0",
    "smart-app-banner": "^2.0.0",
    "smoothscroll-polyfill": "^0.4.4",
    "vue-easy-lightbox": "^1.19.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.7.5",
    "@pinia/nuxt": "^0.9.0",
    "@rollup/plugin-yaml": "^4.1.2",
    "acorn": "^8.14.0",
    "defu": "^6.1.4",
    "eslint": "^9.17.0",
    "fast-xml-parser": "^4.5.1",
    "fs-extra": "^11.2.0",
    "gh-pages": "^6.3.0",
    "node-fetch": "^3.3.2",
    "nuxt": "^3.15.1",
    "pinia": "^2.3.0",
    "qrcode": "^1.5.4",
    "sass": "^1.83.0",
    "sharp": "^0.34.5"
  }
}
